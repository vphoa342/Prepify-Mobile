{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@services": [
        "src/services"
      ],
      "@configs": [
        "src/configs"
      ],
      "@components": [
        "src/components"
      ],
      "@pages": [
        "src/screens"
      ],
      "@hooks": [
        "src/hooks"
      ],
    },
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}